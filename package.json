{
  "name": "terminaltxt-helloworld",
  "version": "0.0.0",
  "description": "WIP - Example project using TerminalTXT.",
  "main": "index.js",
  "scripts": {
    "start": "parcel ./src/index.html",
    "404": "parcel ./src/404.html",
    "build": "rm -rf dist && parcel build ./src/index.html ./src/404.html --public-url /terminaltxt-helloworld",
    "deploy": "gh-pages -d ./dist",
    "build-deploy": "npm run build && npm run deploy"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/figraham/terminaltxt-helloworld.git"
  },
  "author": "Fi Graham",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/figraham/terminaltxt-helloworld/issues"
  },
  "homepage": "https://github.com/figraham/terminaltxt-helloworld#readme",
  "devDependencies": {
    "gh-pages": "^2.0.1",
    "parcel-bundler": "^1.12.3"
  },
  "dependencies": {
    "terminaltxt": "0.0.2"
  }
}
